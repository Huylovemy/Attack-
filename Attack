if game.PlaceId == 2753915549 or game.PlaceId == 4442272183 or game.PlaceId == 7449423635 then
    -- Blox Fruit game IDs
    
    local Player = game:GetService("Players").LocalPlayer
    local Mouse = Player:GetMouse()
    local UIS = game:GetService("UserInputService")
    
    -- Configuration
    local AttackSpeed = 0.1 
    local SuperMode = false 
    local AutoClick = false
    
    -- Táº¡o GUI
    local ScreenGui = Instance.new("ScreenGui")
    local Frame = Instance.new("Frame")
    local Title = Instance.new("TextLabel")
    local ToggleBtn = Instance.new("TextButton")
    local AutoBtn = Instance.new("TextButton")
    local SpeedLabel = Instance.new("TextLabel")
    local SpeedBox = Instance.new("TextBox")
    
    ScreenGui.Name = "SuperM1Gui"
    ScreenGui.Parent = game:GetService("CoreGui")
    
    Frame.Name = "MainFrame"
    Frame.Parent = ScreenGui
    Frame.BackgroundColor3 = Color3.fromRGB(30, 30, 40)
    Frame.BorderSizePixel = 2
    Frame.BorderColor3 = Color3.fromRGB(0, 150, 255)
    Frame.Position = UDim2.new(0.8, 0, 0.3, 0)
    Frame.Size = UDim2.new(0, 200, 0, 180)
    Frame.Active = true
    Frame.Draggable = true
    
    Title.Name = "Title"
    Title.Parent = Frame
    Title.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
    Title.Size = UDim2.new(1, 0, 0, 30)
    Title.Text = "SUPER M1 ATTACK"
    Title.TextColor3 = Color3.fromRGB(0, 200, 255)
    Title.Font = Enum.Font.GothamBold
    Title.TextSize = 14
    
    ToggleBtn.Name = "ToggleBtn"
    ToggleBtn.Parent = Frame
    ToggleBtn.Position = UDim2.new(0.1, 0, 0.2, 0)
    ToggleBtn.Size = UDim2.new(0.8, 0, 0, 35)
    ToggleBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 60)
    ToggleBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    ToggleBtn.Font = Enum.Font.Gotham
    ToggleBtn.TextSize = 12
    ToggleBtn.Text = "SUPER MODE: OFF"
    
    AutoBtn.Name = "AutoBtn"
    AutoBtn.Parent = Frame
    AutoBtn.Position = UDim2.new(0.1, 0, 0.45, 0)
    AutoBtn.Size = UDim2.new(0.8, 0, 0, 35)
    AutoBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 60)
    AutoBtn.TextColor3 = Color3.fromRGB(255, 255, 255)
    AutoBtn.Font = Enum.Font.Gotham
    AutoBtn.TextSize = 12
    AutoBtn.Text = "AUTO CLICK: OFF"
    
    SpeedLabel.Name = "SpeedLabel"
    SpeedLabel.Parent = Frame
    SpeedLabel.Position = UDim2.new(0.1, 0, 0.7, 0)
    SpeedLabel.Size = UDim2.new(0.4, 0, 0, 25)
    SpeedLabel.BackgroundTransparency = 1
    SpeedLabel.Text = "Speed:"
    SpeedLabel.TextColor3 = Color3.fromRGB(200, 200, 200)
    SpeedLabel.Font = Enum.Font.Gotham
    SpeedLabel.TextSize = 12
    
    SpeedBox.Name = "SpeedBox"
    SpeedBox.Parent = Frame
    SpeedBox.Position = UDim2.new(0.55, 0, 0.7, 0)
    SpeedBox.Size = UDim2.new(0.35, 0, 0, 25)
    SpeedBox.BackgroundColor3 = Color3.fromRGB(40, 40, 50)
    SpeedBox.TextColor3 = Color3.fromRGB(255, 255, 255)
    SpeedBox.Font = Enum.Font.Gotham
    SpeedBox.TextSize = 12
    SpeedBox.Text = "0.1"
    SpeedBox.PlaceholderText = "Speed"
    
    -- Function Ä‘á»ƒ thá»±c hiá»‡n táº¥n cĂ´ng
    local function performAttack()
        if SuperMode then
            -- MĂ´ phá»ng click chuá»™t vá»›i tá»‘c Ä‘á»™ cao
            for i = 1, 5 do -- 5 láº§n táº¥n cĂ´ng nhanh
                task.wait(AttackSpeed / 5)
                -- Gá»­i sá»± kiá»‡n click (giáº£ láº­p)
                if Player.Character and Player.Character:FindFirstChild("Humanoid") then
                    -- KĂ­ch hoáº¡t animation táº¥n cĂ´ng
                    local args = {
                        [1] = "mouse1",
                        [2] = Mouse.Hit
                    }
                    
                    -- TĂ¬m remote event Ä‘á»ƒ gá»­i táº¥n cĂ´ng
                    local remotes = game:GetService("ReplicatedStorage"):FindFirstChild("Remotes")
                    if remotes then
                        local combat = remotes:FindFirstChild("Combat")
                        if combat then
                            combat:FireServer(unpack(args))
                        end
                    end
                end
            end
        end
    end
    
    -- Toggle Super Mode
    ToggleBtn.MouseButton1Click:Connect(function()
        SuperMode = not SuperMode
        if SuperMode then
            ToggleBtn.Text = "SUPER MODE: ON"
            ToggleBtn.BackgroundColor3 = Color3.fromRGB(0, 150, 0)
            print("Super M1 Mode: ON")
        else
            ToggleBtn.Text = "SUPER MODE: OFF"
            ToggleBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 60)
            print("Super M1 Mode: OFF")
        end
    end)
    
    -- Toggle Auto Click
    AutoBtn.MouseButton1Click:Connect(function()
        AutoClick = not AutoClick
        if AutoClick then
            AutoBtn.Text = "AUTO CLICK: ON"
            AutoBtn.BackgroundColor3 = Color3.fromRGB(150, 0, 0)
            print("Auto Click: ON")
        else
            AutoBtn.Text = "AUTO CLICK: OFF"
            AutoBtn.BackgroundColor3 = Color3.fromRGB(50, 50, 60)
            print("Auto Click: OFF")
        end
    end)
    
    -- Update attack speed
    SpeedBox.FocusLost:Connect(function()
        local num = tonumber(SpeedBox.Text)
        if num and num > 0 then
            AttackSpeed = math.min(num, 1.0) -- Giá»›i háº¡n tá»‘c Ä‘á»™
            SpeedBox.Text = tostring(AttackSpeed)
            print("Attack Speed set to: " .. AttackSpeed)
        else
            SpeedBox.Text = "0.1"
            AttackSpeed = 0.1
        end
    end)
    
    -- Tá»± Ä‘á»™ng táº¥n cĂ´ng khi AutoClick báº­t
    spawn(function()
        while true do
            if AutoClick and SuperMode then
                performAttack()
            end
            task.wait(AttackSpeed)
        end
    end)
    
    -- Táº¥n cĂ´ng khi click chuá»™t
    UIS.InputBegan:Connect(function(input, gameProcessed)
        if not gameProcessed then
            if input.UserInputType == Enum.UserInputType.MouseButton1 and SuperMode then
                if not AutoClick then -- Chá»‰ táº¥n cĂ´ng náº¿u AutoClick táº¯t
                    performAttack()
                end
            end
            
            -- PhĂ­m táº¯t (VĂ­ dá»¥: F Ä‘á»ƒ báº­t/táº¯t)
            if input.KeyCode == Enum.KeyCode.F then
                SuperMode = not SuperMode
                ToggleBtn.Text = SuperMode and "SUPER MODE: ON" or "SUPER MODE: OFF"
                ToggleBtn.BackgroundColor3 = SuperMode and Color3.fromRGB(0, 150, 0) or Color3.fromRGB(50, 50, 60)
            end
        end
    end)
    
    print("Super M1 Attack Script loaded successfully!")
    print("Controls:")
    print("- Click Toggle button to enable/disable")
    print("- F key to toggle super mode")
    print("- Adjust speed in the text box")
    
else
    warn("This script only works in Blox Fruit!")
end
